{
    "name": "valentin-morice/filament-html-output",
    "description": "A Filament form component that allows you to display HTML without creating views",
    "keywords": [
        "valentin-morice",
        "laravel",
        "filament-html-output"
    ],
    "homepage": "https://github.com/valentinmorice/filament-html-output",
    "support": {
        "issues": "https://github.com/valentinmorice/filament-html-output/issues",
        "source": "https://github.com/valentinmorice/filament-html-output"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "valentin-morice",
            "email": "valentinmorice1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ValentinMorice\\HtmlOutput\\": "src/",
            "ValentinMorice\\HtmlOutput\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ValentinMorice\\HtmlOutput\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ValentinMorice\\HtmlOutput\\HtmlOutputServiceProvider"
            ],
            "aliases": {
                "HtmlOutput": "ValentinMorice\\HtmlOutput\\Facades\\HtmlOutput"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
